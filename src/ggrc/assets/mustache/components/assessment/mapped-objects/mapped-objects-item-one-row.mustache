<div class="assessment-mapped-objects__item {{#if instance.isSelected}}active{{/if}}" data-object-id="{{instance.id}}" can-click="addSelection"
    data-object-type="{{instance.class.table_singular}}">
    <div class="assessment-mapped-objects__item-body" >
        <div class="title">
            <i class="fa fa-{{instance.class.table_singular}}"></i>
          {{{firstnonempty instance.title instance.description_inline instance.name instance.email ''}}}
        </div>
    </div>
</div>
